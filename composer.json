{
    "name": "stevebauman/laravel-husk",
    "description": "A Laravel Dusk environment for your Javascript only applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^10.0",
        "nunomaduro/laravel-console-dusk": "^1.11",
        "nunomaduro/termwind": "^1.15"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/dusk": "^7.9",
        "mockery/mockery": "^1.6.6",
        "pestphp/pest": "^2.16"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
